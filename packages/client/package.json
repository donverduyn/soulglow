{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "yarn playwright:install && patch-package && yarn api:generate && yarn codegen:client --no-external",
    "bootstrap:ci": "patch-package && yarn api:generate && yarn codegen:client --no-external",
    "playwright:install": "yarn playwright install --with-deps --only-shell chromium",
    "api:generate": "rm -rf ./src/__generated && tsx -e \"import { generate } from './scripts/api-gen'; generate('./openapi.yml', './src/__generated/api');\"",
    "dev": "vite --open",
    "debug": "INTERNAL_CHROMIUM=true vite --debug",
    "build": "vite build && yarn sb:build",
    "lint:eslint": "eslint . --cache --cache-location ./.cache/eslint/.eslintcache",
    "lint:tsc": "tsc -b tsconfig.json",
    "lint:stylelint": "stylelint 'src/**/*.css'",
    "lint": "tsx -e \"import { lint } from './scripts/lint'; lint(['yarn lint:tsc', 'yarn lint:eslint','yarn lint:stylelint']);\"",
    "lint:ci": "tsx -e \"import { lint } from './scripts/lint'; lint(['yarn lint:tsc', 'yarn lint:eslint --cache-strategy content','yarn lint:stylelint']);\"",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:stylelint --fix",
    "preview": "yarn build --profile && tsx -e \"import { viewStatic } from './scripts/browser'; viewStatic('./dist');\"",
    "test": "vitest --ui --no-isolate --pool=threads",
    "test:coverage": "vitest run --coverage --silent",
    "test:e2e": "",
    "analyze": "yarn build && tsx -e \"import { viewStatic } from './scripts/browser'; viewStatic('./.analyzer');\"",
    "clean": "rm -rf dist .vite-inspect .analyzer .swc yarn-error.log coverage dev-dist storybook-static dist .cache",
    "reset:node_modules": "rm -rf node_modules/* node_modules/.[!.]* node_modules/..?*",
    "reset": "yarn clean && yarn reset:node_modules",
    "ci:local": "act -P ubuntu-latest=-self-hosted -j build --cache-server-addr 127.0.0.1",
    "dep:stats": "npx --yes tsx -e \"import { view } from './scripts/dep-stats'; view('./package.json');\"",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "codegen:client": "npx --yes wait-on ./../api/__generated/introspection.json && graphql-codegen --config ./codegen.client.ts",
    "codegen:daemon": "npx --yes wait-on ./../api/__generated/introspection.json && npx --yes wait-on ./../../node_modules/.bin/graphql-codegen && yarn codegen:daemon:run",
    "codegen:daemon:run": "IPC_DAEMON_NAME=codegen IPC_DAEMON_COMMAND=yarn IPC_DAEMON_ARGS='codegen:client --watch' npx --yes tsx scripts/ipc-daemon.ts"
  },
  "dependencies": {
    "@effect-rx/rx": "^0.34.7",
    "@effect/platform": "^0.80.2",
    "@effect/platform-browser": "^0.59.2",
    "@mantine/charts": "^7.17.0",
    "@mantine/core": "^7.17.0",
    "@mantine/form": "^7.17.0",
    "@mantine/hooks": "^7.17.0",
    "@mantine/modals": "^7.17.0",
    "@mantine/notifications": "^7.17.0",
    "@tabler/icons-react": "3.31.0",
    "@urql/core": "^5.1.1",
    "@urql/exchange-graphcache": "^7.2.3",
    "@urql/exchange-populate": "^1.2.1",
    "clsx": "^2.1.1",
    "culori": "^4.0.1",
    "dayjs": "^1.11.11",
    "effect": "^3.14.2",
    "graphql": "^16.10.0",
    "i18next": "24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "microdiff": "^1.5.0",
    "mobx": "^6.13.6",
    "mobx-react-lite": "^4.1.0",
    "mobx-utils": "^6.1.0",
    "moize": "^6.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "15.4.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.2.0",
    "react-use": "^17.6.0",
    "recharts": "^2.15.1",
    "remeda": "^2.21.0",
    "scheduler": "^0.25.0",
    "urql": "^4.2.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.1"
  },
  "browserslist": [
    "defaults"
  ]
}
